import 'dart:io';

void main() {
  print('Ingrese un n√∫mero para mostrar su tabla de multiplicar:');
  String? input = stdin.readLineSync();
  int numero = int.parse(input!);

  print('Tabla de multiplicar del $numero:');
  for (int i = 1; i <= 10; i++) {
    print('$numero x $i = ${numero * i}');
  }
}
